{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "io.nats.nex.v2.node_ping_response",
  "title": "NodePingResponse",
  "type": "object",
  "properties": {
    "node_id": {
      "type": "string",
      "description": "The unique identifier of the node"
    },
    "version": {
      "type": "string",
      "description": "The version of the node"
    },
    "uptime": {
      "type": "string",
      "description": "The uptime of the node"
    },
    "xkey": {
      "type": "string",
      "description": "The target nodes xkey"
    },
    "tags": {
      "$ref": "./shared-tag-map.json",
      "description": "Placement tags associated with node"
    },
    "state": {
      "$ref": "./shared-node-state.json",
      "description": "The state of the node"
    },
    "agent_count": {
      "type": "integer",
      "description": "The number of agents running on the node"
    }
  },
  "required": [
    "node_id",
    "version",
    "uptime",
    "xkey",
    "tags",
    "agent_count",
    "state"
  ],
  "additionalProperties": false
}
